package ex01;

public class Car {
	//필드
	String company="현대";
	String model;
	String color;
	int maxSpeed;
	
	//출력메서드 (동작)
	void printCar(Car c) {
		System.out.println("-------------------------");
		System.out.println("회사명:"+c.company);
		System.out.println("모델명:"+c.model);
		System.out.println("색상:"+c.color);
		System.out.println("최대속도:"+c.maxSpeed);
	}
	
}
