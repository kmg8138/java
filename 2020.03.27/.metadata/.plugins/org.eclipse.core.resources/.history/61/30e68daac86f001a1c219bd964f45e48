package ex01;

public class Sample {

	public static void main(String[] args) {
		
		int score=95;
		//점수가 90점 이상이면 A출력
		//점수가 80점 이상이면 B출력
		//점수가 70점 이상이면 C출력
		//점수가 70점 미만이면 D출력
		
		//100~95 사이면 A+
		//94~90 사이면 A0
		if(score>=100 && score<=95) {
			System.out.println("A+");
		}else if(score<=94 && score>=90) {
			System.out.println("A0");
		}

	}

}
